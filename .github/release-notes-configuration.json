{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "feat", "enhancement"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix", "bugfix", "bug"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "🤖 Maintenance",
      "label": ["chore"]
    },
    {
      "title": "✏️ Docs",
      "label": ["docs"]
    },
    {
      "title": "💡 Refactor",
      "label": ["refactor"]
    },
    {
      "title": "💄 Style",
      "label": ["style"]
    },
    {
      "title": "🎡 CI",
      "label": ["ci"]
    },
    {
      "title": "⚡️ Perf",
      "label": ["perf"]
    }
  ],
  "ignore_labels": [],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- ${{TITLE}} @${{@$AUTHOR}} (#${{NUMBER}})",
  "base_branches": [
    "main"
  ]
}
