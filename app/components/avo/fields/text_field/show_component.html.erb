<%= field_wrapper field: @field,
  resource: @resource,
  index: @index,
  view: view,
  data: wrapper_data do %>
  <% if @field.as_html %>
    <%== @field.value %>
  <% elsif @field.protocol.present? %>
    <%= link_to @field.value, "#{@field.protocol}:#{@field.value}" %>
  <% else %>
    <%= @field.value %>
  <% end %>
<% end %>
